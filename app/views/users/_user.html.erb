<div id="<%= dom_id user %>" class="col">
  <div class="card mb-3">
    <div class="card-header">
      <%= link_to user_path(user) do %>
        <i class="fa fa-link"></i>
        <strong><%= user.to_s %></strong>
      <% end %>
    </div>
    <div class="card-body">
      <p><strong>Full name:</strong> <%= user.full_name %></p>
      <p><strong>Email:</strong> <%= user.email %></p>
      <p><strong>Encrypted password:</strong> <%= user.encrypted_password %></p>
      <p><strong>Role:</strong> <%= user.role %></p>
      <p><strong>Reset password token:</strong> <%= user.reset_password_token %></p>
      <p><strong>Reset password sent at:</strong> <%= user.reset_password_sent_at %></p>
      <p><strong>Remember created at:</strong> <%= user.remember_created_at %></p>
    </div>
  </div>
</div>
